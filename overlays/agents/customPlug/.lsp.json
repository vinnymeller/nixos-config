{
  "basedpyright": {
    "command": "basedpyright-langserver",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".py": "python",
      ".pyi": "python"
    }
  },
  "ccls": {
    "command": "ccls",
    "extensionToLanguage": {
      ".c": "c",
      ".cpp": "cpp",
      ".objc": "objective-c",
      ".objcpp": "objective-cpp",
      ".cuda": "cuda"
    }
  },
  "gopls": {
    "command": "gopls",
    "extensionToLanguage": {
      ".go": "go",
      ".work": "gowork",
      ".mod": "gomod",
      ".gotmpl": "gotmpl"
    }
  },
  "jsonls": {
    "command": "vscode-json-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".json": "json",
      ".jsonc": "jsonc"
    }
  },
  "lua-language-server": {
    "command": "lua-language-server",
    "extensionToLanguage": {
      ".lua": "lua"
    }
  },
  "nixd": {
    "command": "nixd",
    "extensionToLanguage": {
      ".nix": "nix"
    }
  },
  "rust-analyzer": {
    "command": "rust-analyzer",
    "extensionToLanguage": {
      ".rs": "rust"
    }
  },
  "taplo": {
    "command": "taplo",
    "args": ["lsp", "stdio"],
    "extensionToLanguage": {
      ".toml": "toml"
    }
  },
  "terraform-ls": {
    "command": "terraform-ls",
    "args": ["serve"],
    "extensionToLanguage": {
      ".tf": "terraform",
      ".tfvars": "terraform-vars"
    }
  },
  "typescript-language-server": {
    "command": "typescript-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".ts": "typescript",
      ".tsx": "typescriptreact",
      ".js": "javascript",
      ".jsx": "javascriptreact",
      ".mts": "typescript",
      ".cts": "typescript",
      ".mjs": "javascript",
      ".cjs": "javascript"
    }
  },
  "yaml-language-server": {
    "command": "yaml-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".yaml": "yaml",
      ".yml": "yaml"
    }
  }
}
