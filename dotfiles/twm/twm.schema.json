{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RawTwmGlobal",
  "type": "object",
  "properties": {
    "exclude_path_components": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "layouts": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/LayoutDefinition"
      }
    },
    "max_search_depth": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint",
      "minimum": 0.0
    },
    "search_paths": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "session_name_path_components": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint",
      "minimum": 0.0
    },
    "workspace_definitions": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/WorkspaceDefinitionConfig"
      }
    }
  },
  "definitions": {
    "LayoutDefinition": {
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "commands": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "inherits": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "name": {
          "type": "string"
        }
      }
    },
    "WorkspaceDefinitionConfig": {
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "default_layout": {
          "type": [
            "string",
            "null"
          ]
        },
        "has_all_files": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "has_any_file": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "missing_all_files": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "missing_any_file": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "name": {
          "type": "string"
        }
      }
    }
  }
}
