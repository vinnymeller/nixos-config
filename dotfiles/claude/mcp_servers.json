{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "nix",
      "args": ["run", "github:akirak/nix-playwright-mcp"],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {}
    },
    "github": {
      "type": "stdio",
      "command": "nix",
      "args": ["run", "nixpkgs#github-mcp-server", "--", "stdio"],
      "env": {}
    },
    "zen": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/BeehiveInnovations/zen-mcp-server.git",
        "zen-mcp-server"
      ],
      "env": {}
    }
  }
}
